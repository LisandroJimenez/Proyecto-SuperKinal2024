<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1" fx:controller="org.lisandroJimenez.controller.MenuProductosController">
   <children>
      <ImageView fitHeight="400.0" fitWidth="600.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/CargarImagen.png" />
         </image>
      </ImageView>
      <ImageView fx:id="imgCargar" fitHeight="233.0" fitWidth="217.0" layoutX="189.0" layoutY="125.0" onDragDropped="#handleOnDrop" onDragOver="#handleOnDrag" pickOnBounds="true" preserveRatio="true" />
      <Button fx:id="btnCargar" layoutX="221.0" layoutY="368.0" mnemonicParsing="false" onAction="#handleButtonAction" text="Guardar" />
      <Button fx:id="btnCancelar" layoutX="335.0" layoutY="368.0" mnemonicParsing="false" onAction="#handleButtonAction" text="Cancelar" />
   </children>
</AnchorPane>
