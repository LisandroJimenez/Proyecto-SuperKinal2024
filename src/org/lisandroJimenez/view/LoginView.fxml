<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="625.0" prefWidth="450.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.lisandroJimenez.controller.LoginController">
   <children>
      <ImageView fitHeight="650.0" fitWidth="450.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/Login.png" />
         </image>
      </ImageView>
      <TextField fx:id="tfUser" layoutX="52.9" layoutY="257.8" prefHeight="40.7" prefWidth="243.9" />
      <PasswordField fx:id="tfPassword" layoutX="53.0" layoutY="366.0" prefHeight="40.7" prefWidth="243.9" />
      <Button fx:id="btnIniciar" layoutX="175.0" layoutY="481.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="32.0" prefWidth="100.0" text="Iniciar Sesion" />
      <Button fx:id="btnRegistrar" disable="true" layoutX="175.0" layoutY="528.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="32.0" prefWidth="100.0" text="Registrar" />
   </children>
</AnchorPane>
